{
    "name": "FreshChat Notify",
    "version": "0.0.2",
    "description": "Notifies on new FreshChat messages",
    "background": {
        "scripts": ["bg.js"]
    },
    "manifest_version": 2,
    "icons": {
        "128": "icon.png"
    },
    "permissions": ["<all_urls>", "tabs"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "FreshChat Notify"
    }
}